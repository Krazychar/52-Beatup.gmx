else if(attacking=1 &amp;&amp; grounded=1) hsp=0
//horizontal collision   
if place_meeting(x+hsp,y,ground)
{
    if(!place_meeting(x+sign(hsp),y,ground)) hsp+=sign(hsp);
    hsp = 0;
}    
 
//commit to movement
if(gal.hitStop=false)
{
x += hsp;
y += vsp;

vsp+=grav
}


